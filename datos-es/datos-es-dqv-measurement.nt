@prefix : <http://www.example.org/#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dqv: <http://www.w3.org/ns/dqv#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ldqv: <https://www.w3.org/2016/05/ldqd> .

:myDatosDataset 
    a dcat:Dataset ;
    dcterms:title "datos.gob.es" ; 
    dcat:distribution :myDatosSPARQLDatasetDistribution
    .
	
:myDatosSPARQLDatasetDistribution
    a dcat:Distribution ;
    dcat:accessURL <https://datos.gob.es/virtuoso/sparql>
    dcterms:title "SPARQL access to the datos.gob.es dataset" ;
    dcat:mediaType "application/sparql-results+json"  ;
    dqv:hasQualityMeasurement :myDatosMeasurement1
    .

:ShExConsistencyMetric
    a dqv:Metric ;
    skos:definition "It checks the ratio of conformant resources according to a Shape Expression schema."@en ;
    dqv:inDimension ldqv:consistency ;
    dqv:expectedDataType xsd:decimal
    .

:myDatosMeasurement1 
    a dqv:QualityMeasurement ;
    dqv:computedOn :myDatosDatasetDistribution ;
    dqv:isMeasurementOf :ShExConsistencyMetric ;
    dqv:value "0.77"^^xsd:decimal 
    .
